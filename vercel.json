{
  "version": 2,
  "builds": [
    {
      "src": "api.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api-keys",
      "dest": "/api.js"
    },
    {
      "src": "/players",
      "dest": "/api.js"
    }
  ],
  "git": {
    "deploymentEnabled": {
      "gh-pages": false
    }
  }
}
