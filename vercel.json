{
  "version": 2,
  "builds": [
    {
      "src": "api.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api-keys",
      "methods": ["GET"],
      "dest": "/api.js"
    },
    {
      "src": "/players",
      "methods": ["GET"],
      "dest": "/api.js"
    },
    {
      "src": "/leaderboard-data",
      "methods": ["GET"],
      "dest": "/api.js"
    },
    {
      "src": "/fetch-picks",
      "methods": ["GET"],
      "dest": "/api.js"
    },
    {
      "src": "/submit-picks",
      "methods": ["GET"],
      "dest": "/api.js"
    },

    {
      "src": "/golfers",
      "methods": ["GET"],
      "dest": "/api.js"
    }
  ]
}
